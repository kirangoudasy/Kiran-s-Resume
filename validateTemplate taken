// STEP-1 : use same ID and INSERT it explicitly

const key = req.data.ID;

if (!key) {
  return req.error(400, "ID is mandatory for validation");
}

await cds.transaction(req).run(
  INSERT.into('com.scb.fileupload.master.DataValidator').entries({
    ID: key,
    JSON_DATA: jsonData
  })
);

console.log("Validation ID:", key);
